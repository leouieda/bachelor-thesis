\begin_layout Columns

\size footnotesize
\begin_inset Formula \[
T_{xx}=\dfrac{1}{r^{2}}\left(\dfrac{\partial^{2}V}{\partial\varphi^{2}}+r\dfrac{\partial V}{\partial r}\right)\]

\end_inset


\end_layout

\begin_layout Columns

\size footnotesize
\begin_inset Formula \[
T_{xy}=\dfrac{1}{r^{2}\cos\varphi}\left(\dfrac{\partial^{2}V}{\partial\varphi\partial\lambda}+\tan\varphi\dfrac{\partial V}{\partial\lambda}\right)=T_{yx}\]

\end_inset


\end_layout

\begin_layout Columns

\size footnotesize
\begin_inset Formula \[
T_{xz}=\dfrac{1}{r}\left(\dfrac{\partial^{2}V}{\partial\varphi\partial r}-\dfrac{1}{r}\dfrac{\partial V}{\partial\varphi}\right)=T_{zx}\]

\end_inset


\end_layout

