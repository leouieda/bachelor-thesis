\end_layout

\begin_layout Plain Layout

f(x,y,z)
\backslash
 dx
\backslash
 dy
\backslash
 dz  
\backslash
approx     
\end_layout

\begin_layout Plain Layout


\backslash
dfrac{(x_2 - x_1)(y_2 - y_1)(z_2 - z_1)}{8}     
\backslash

\backslash

\end_layout

\begin_layout Plain Layout


\backslash
sum
\backslash
limits_{i=1}^{N_x}    
\end_layout

\begin_layout Plain Layout


\backslash
sum
\backslash
limits_{j=1}^{N_y}     
\end_layout

\begin_layout Plain Layout


\backslash
