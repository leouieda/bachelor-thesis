\begin_layout Standard
Cálculo dos pontos de discretização (nós) da QGL
\end_layout

\begin_layout Itemize
Método de Newton Modificado (BARRERA-FIGUEROA 
\shape italic
et al.
\shape default
, 2006)
\begin_inset Formula \[
\xi_{k,n+1}=\xi_{k,n}-\dfrac{P_{N}(\xi_{k,n})}{P_{N}'(\xi_{k,n})-P_{N}(\xi_{k,n}){\displaystyle \sum\limits _{i=1}^{k-1}\dfrac{1}{\xi_{k,n}-\xi_{i}}}}\]

\end_inset


\end_layout

\begin_layout Itemize
Chute inicial de Press 
\shape italic
et al.

\shape default
 (1992)
\begin_inset Formula \[
\xi_{k,0}=\dfrac{\cos\left(\pi k-\frac{\pi}{4}\right)}{N+\frac{1}{2}}\]

\end_inset


\end_layout

