\begin_layout FrameSubtitle
TGG em coordenadas esféricas
\end_layout

\begin_layout Standard
TGG é um tensor com 9 elementos:
\end_layout

\begin_layout Standard
\begin_inset Formula \[
T=\begin{pmatrix}T_{xx} & T_{xy} & T_{xz}\\
T_{yx} & T_{yy} & T_{yz}\\
T_{zx} & T_{zy} & T_{zz}\end{pmatrix}=\begin{pmatrix}\dfrac{\partial^{2}V}{\partial x^{2}} & \dfrac{\partial^{2}V}{\partial x\partial y} & \dfrac{\partial^{2}V}{\partial x\partial z}\\[4mm]
\dfrac{\partial^{2}V}{\partial y\partial x} & \dfrac{\partial^{2}V}{\partial y^{2}} & \dfrac{\partial^{2}V}{\partial y\partial z}\\[4mm]
\dfrac{\partial^{2}V}{\partial z\partial x} & \dfrac{\partial^{2}V}{\partial z\partial y} & \dfrac{\partial^{2}V}{\partial z^{2}}\end{pmatrix}\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
hspace{
\backslash
fill}
\backslash

\backslash
[0.5cm]
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $T$
\end_inset

 é simétrico e possui 5 elementos independentes.
\end_layout

\begin_layout EndFrame

\end_layout

