\begin_layout Standard
\begin_inset Formula \[
\mathbf{e}^{L_{P}}=\underbrace{\mathbf{P}_{2}\mathbf{R}_{2}\left(\varphi_{P}-\dfrac{\pi}{2}\right)\mathbf{R}_{3}\left(\lambda_{P}-\lambda_{Q}\right)\mathbf{R}_{2}\left(\dfrac{\pi}{2}-\varphi_{Q}\right)\mathbf{P}_{2}}_{\mathbf{R}^{QP}}\mathbf{e}^{L_{Q}}\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \[
\mathbf{R}^{QP}=\mathbf{P}_{2}\mathbf{R}_{2}\left(\varphi_{P}-\dfrac{\pi}{2}\right)\mathbf{R}_{3}\left(\lambda_{P}-\lambda_{Q}\right)\mathbf{R}_{2}\left(\dfrac{\pi}{2}-\varphi_{Q}\right)\mathbf{P}_{2}\]

\end_inset


